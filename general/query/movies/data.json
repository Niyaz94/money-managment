{
    "decsrition":{
        "type":{
            "num":"number value",
            "str":"string value",
            "var":"add the variable",
            "ref":"conect two tables",
            "fun":"calling the function",
            "sub":"Calling subquery"
        }
    },
    "category":{
        "select":["id","name","orders"],
        "from":["moviecategory"],
        "where":[
            {"key":"deleted","operation":"=" ,"value":0,"type":"num","link":"And","group":[1,"And"]}
        ],
        "order":[["orders","DESC"]],
        "subquery":{}
    },
    "tag":{
        "select":["id"],
        "from":["Movies"],
        "where":[
            {"key":"deleted","operation":"=" ,"value":0,"type":"num","link":"And","group":[1,"And"]},
            {"key":"id","operation":"<>" ,"value":"id","type":"var","link":"And","group":[1,"And"]},
            {"key":"movieTagId","operation":"=" ,"value":"findTagId","type":"sub","link":"And","group":[1,"And"]}
        ],
        "order":[["id","DESC"]],
        "subquery":{
            "findTagId":{
                "select":["movieTagId"],
                "from":["Movies"],
                "where":[
                    {"key":"deleted","operation":"=" ,"value":0,"type":"num","link":"And","group":[1,"And"]},
                    {"key":"id","operation":"=" ,"value":"id","type":"var","link":"And","group":[1,"And"]}
                ],
                "order":[]
            }
        }
    }
}
